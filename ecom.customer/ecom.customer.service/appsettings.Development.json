{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "KeyVault": {
    "Enabled": false,
    "Endpoint": ""
  },
  "CorsPolicy": {
    "Origins": [
      "http://localhost:3000",
      "http://localhost:4200",
      "http://localhost:5000"
    ]
  },
  "JwtSettings": {
    "Authority": "http://localhost:5000",
    "Audience": "http://localhost:5000",
    "SecretKey": "your-development-secret-key-min-16-chars",
    "TokenExpirationInMinutes": 60
  },
  "AppSettings": {
    "UseLocalSecrets": true
  }
}
