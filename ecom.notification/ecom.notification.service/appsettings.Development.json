{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "CorsPolicy": {
    "Origins": [
      "http://localhost:3000",
      "http://localhost:5000"
    ]
  },
  "JwtSettings": {
    "RequireHttpsMetadata": false
  },
  "DaprSettings": {
    "UseStateStore": true,
    "StateStoreName": "statestore"
  }
}
